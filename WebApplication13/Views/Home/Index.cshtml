@model List<WebApplication13.Models.User>
@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>
<table border="1" cellpadding="4" align="center">
    <tr align="center">
        <td><h3>Id</h3></td>
        <td><h3>Name</h3></td>
        <td><h3>Email</h3></td>
    </tr>
    @foreach (var row in Model)
    {
        <tr align="center">
            <td>@row.Id</td>
            <td>@row.Name</td>
            <td>@row.Email</td>
        </tr>
    }
</table>
